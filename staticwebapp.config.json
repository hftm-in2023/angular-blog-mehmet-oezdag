{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/*.{png,jpg,gif,ico}",
      "/*.{js,css,map}",
      "/assets/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "cache-control": "public, max-age=3600",
    "x-frame-options": "SAMEORIGIN"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".html": "text/html"
  }
}
